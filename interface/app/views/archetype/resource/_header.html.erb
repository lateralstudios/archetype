<% if key = flash.keys.first %>
  <div class="alert alert-<%= key == 'alert' ? 'danger' : 'info' %> alert-dismissible">
    <button type="button" class="close" data-dismiss="alert" aria-hidden="true">Ã—</button>
    <%= flash[key] %>
  </div>
<% end %>
<!-- Content Header (Page header) -->
<section class="content-header">
  <h1>
    <%= raw resourceful.title %>
  </h1>
  <%= render 'breadcrumbs', breadcrumb: interface.breadcrumbs %>
</section>
